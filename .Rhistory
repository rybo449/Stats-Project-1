if (length(vv)==0)
{
ff[[1]]<-"BIRTHWT~1"
}
else if (length(vv)==1)
{
ff[[j]]<-paste("BIRTHWT~",vv[1], sep="")
}
else if (length(vv)>=2) {
ff[[j]]<-paste("BIRTHWT~",vv[1], sep="")
